pipeline { 
    agent any 
 
    stages { 
        stage('Checkout Code') { 
            steps { 
                git branch: 'main',
                    url: 'https://github.com/Jayanise0703/Simpleapp.git' 
            } 
        } 
 
        stage('Print Message') { 
            steps { 
                echo 'Hello! Jenkins Pipeline executed successfully' 
            } 
        } 
    } 
} 
